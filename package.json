{
  "name": "kobotoolbox.org",
  "private": true,
  "version": "2.0.0",
  "repository": "https://github.com/kobotoolbox/kobotoolbox.org.git",
  "bugs": "https://github.com/kobotoolbox/kobotoolbox.org/issues",
  "scripts": {
    "copy-images": "node ./_scripts/copy-images.js",
    "postinstall": "npm run copy-images",
    "jekyll-build": "bundle exec jekyll build",
    "jekyll-serve": "bundle exec jekyll serve --watch --baseurl ''",
    "assets-watch": "parcel watch _assets/styles/kobo.scss _assets/scripts/kobo.ts --dist-dir assets",
    "assets-build": "parcel build _assets/styles/kobo.scss _assets/scripts/kobo.ts --dist-dir assets"
  },
  "dependencies": {
    "kobo-common": "https://github.com/kobotoolbox/kobo-common.git#semver:1.15.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.16.7",
    "parcel": "^2.3.2",
    "postcss": "^8.4.7",
    "typescript": "^4.6.2"
  }
}
